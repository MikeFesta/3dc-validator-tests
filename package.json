{
  "name": "@mikefesta/3dc-validator-tests",
  "version": "1.0.0-alpha.9",
  "author": "Mike Festa",
  "description": "A collection of 3D models, schemas, and unit tests for the 3dc-validator npm package. Stored as a separate repo to keep large binaries out of the main project.",
  "license": "SPDX-License-Identifier: Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MikeFesta/3dc-validator-tests"
  },
  "type": "module",
  "esm": true,
  "keywords": [
    "3d",
    "asset",
    "commerce",
    "glb",
    "gltf",
    "khronos",
    "validator",
    "tests"
  ],
  "scripts": {
    "test": "mocha -n loader=ts-node/esm 'tests/**/*.ts'",
    "test-models": "mocha -n loader=ts-node/esm 'model-tests/**/*.ts'",
    "test-by-name": "mocha -n loader=ts-node/esm 'tests/**/*.ts' --grep"
  },
  "devDependencies": {
    "@mikefesta/3dc-validator": "*",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
